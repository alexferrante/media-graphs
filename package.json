{
  "name": "Embellist",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "Generate cover images for your Spotify playlists",
  "author": "Alex Ferrante",
  "license": "",
  "scripts": {
    "start": "webpack-dev-server --open --port 3000 --compress --history-api-fallback --mode development",
    "prebuild": "rimraf dist",
    "build": "webpack --mode production --progress",
